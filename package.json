{
  "name": "oscee",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "pretty": "prettier --write src/*",
    "check-config": "ajv -s src/schema/config.schema.json -r 'src/schema/trigger/*.json' -r 'src/schema/action/*.json' -d src/config.json",
    "dev": "nodemon src/index.js src/config-dev.json",
    "package": "pkg . -t host"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.12.0",
    "lodash": "^4.17.21",
    "midi": "^2.0.0",
    "osc-min": "^1.1.2"
  },
  "devDependencies": {
    "ajv-cli": "^5.0.0",
    "nodemon": "^2.0.22",
    "pkg": "^5.8.1",
    "prettier": "^2.8.8"
  },
  "pkg": {
    "scripts": "src/*.js",
    "assets": [
      "node_modules/midi"
    ],
    "outputPath": "dist"
  }
}
