{
  "name": "showbridge",
  "version": "0.0.0",
  "description": "Simple Protocol Router /s",
  "bin": "main.js",
  "scripts": {
    "pretty": "prettier --write lib/*",
    "test:lib": "node tests/test.js",
    "prebuild": "rimraf dist/bin",
    "bundle:dev": "ncc build main.js -o dist/bundle && cp -r lib/node_modules/@julusian/midi/prebuilds dist/bundle",
    "dev": "nodemon --exec 'node main.js -d -c config/dev.json --webui dist/webui'",
    "start": "node main.js"
  },
  "author": {
    "name": "Joel Wetzell",
    "email": "me@jwetzell.com",
    "url": "https://jwetzell.com"
  },
  "repository": "https://github.com/jwetzell/showbridge",
  "keywords": [
    "show",
    "control",
    "protocol",
    "router",
    "theatre"
  ],
  "license": "GPL-3.0-only",
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
