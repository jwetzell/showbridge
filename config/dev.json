{
  "$schema": "../schema/config.schema.json",
  "http": {
    "params": {
      "port": 3000
    },
    "triggers": [
      {
        "type": "regex",
        "params": {
          "patterns": ["test"],
          "properties": ["originalUrl"]
        },
        "actions": [
          {
            "type": "osc-output",
            "params": {
              "address": "/test",
              "args": [],
              "host": "127.0.0.1",
              "port": 8000,
              "protocol": "udp"
            },
            "enabled": true
          },
          {
            "type": "log",
            "transforms": [
              {
                "type": "template",
                "params": {
                  "property": "originalUrl",
                  "template": "hello"
                },
                "enabled": true
              }
            ],
            "enabled": true
          }
        ],
        "enabled": true
      },
      {
        "type": "regex",
        "params": {
          "patterns": ["hello"],
          "properties": ["originalUrl"]
        },
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "ws": {
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "osc": {
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "midi": {
    "params": {
      "virtualInputName": "showbridge dev Input",
      "virtualOutputName": "showbridge dev Output"
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "tcp": {
    "params": {
      "port": 8001
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "udp": {
    "params": {
      "port": 8001
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "mqtt": {
    "params": {
      "broker": "",
      "topics": ["#"]
    },
    "triggers": [
      {
        "type": "any",
        "actions": [{ "type": "log", "enabled": true }],
        "enabled": true
      }
    ]
  },
  "cloud": {
    "params": {
      "url": "",
      "rooms": ["example-room"]
    }
  }
