{
  "http": {
    "params": {
      "port": 3000
    },
    "triggers": [
      {
        "type": "regex",
        "params": {
          "patterns": [
            "/test.*"
          ],
          "properties": [
            "originalUrl"
          ]
        },
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "subTriggers": [
          {
            "type": "regex",
            "params": {
              "patterns": [
                "/testing"
              ],
              "properties": [
                "originalUrl"
              ]
            },
            "actions": [
              {
                "type": "log",
                "transforms": [],
                "enabled": true
              },
              {
                "type": "midi-output",
                "params": {
                  "_port": "${vars.patches.midi[1].port}",
                  "status": "note_on",
                  "channel": 1,
                  "note": 6,
                  "velocity": 6
                },
                "transforms": [],
                "enabled": true
              },
              {
                "type": "osc-output",
                "params": {
                  "_host": "${vars.patches.network[0].host}",
                  "_port": "${vars.patches.network[0].port}",
                  "protocol": "udp",
                  "address": "/test",
                  "_args": [],
                  "args": []
                },
                "transforms": [],
                "enabled": true
              }
            ],
            "subTriggers": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "ws": {
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "osc": {
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "midi": {
    "params": {
      "virtualInputName": "showbridge dev Input",
      "virtualOutputName": "showbridge dev Output"
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "tcp": {
    "params": {
      "port": 8001
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "udp": {
    "params": {
      "port": 8001
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "mqtt": {
    "params": {
      "broker": "",
      "topics": [
        "#"
      ]
    },
    "triggers": [
      {
        "type": "any",
        "actions": [
          {
            "type": "log",
            "transforms": [],
            "enabled": true
          }
        ],
        "enabled": true
      }
    ]
  },
  "cloud": {
    "params": {
      "url": "https://cloud.showbridge.io",
      "rooms": [
        "dev"
      ]
    }
  }
}